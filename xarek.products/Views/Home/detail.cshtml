<div class="col-8">
    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade" id="list-home" role="tabpanel" aria-labelledby="list-home-list">
            <table>
                <tr>
                    <td><p>Идентификатор продукта: </p> </td>
                    <td><input type="text" ng-model="ProductId" role="tab" ng-change="change(ProductId)" /></td>
                </tr>
                <tr>
                    <td><p>Наименование продукта: </p> </td>
                    <td><input type="text" ng-model="ProductName" role="tab" /></td>
                </tr>
                <tr>
                    <td><p>Количество товара: </p> </td>
                    <td><input type="text" ng-model="Count" role="tab" /></td>
                </tr>
                <tr>
                    <td><p>Цена продукта: </p> </td>
                    <td><input type="text" ng-model="Price" role="tab" /></td>
                </tr>
            </table>
            <button class="btn btn-outline-info" ng-click="Save()" role="tab" ng-model="butt">Записать</button>

        </div>
    </div>
</div>